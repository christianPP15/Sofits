<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="16dp"
    tools:context=".ui.MiPerfil.Informacion.InformacionUsuarioFragment">


    <ImageView
        android:id="@+id/imageView_estrella3_valoracionPropia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@android:drawable/btn_star"
        app:layout_constraintBottom_toBottomOf="@+id/imageView_estrella2_valoracionPropia"
        app:layout_constraintEnd_toStartOf="@+id/imageView_estrella4_valoracionPropia"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_estrella2_valoracionPropia"
        app:layout_constraintTop_toTopOf="@+id/imageView_estrella2_valoracionPropia" />

    <ImageView
        android:id="@+id/imageView_estrella2_valoracionPropia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@android:drawable/btn_star"
        app:layout_constraintBottom_toBottomOf="@+id/imageView_estrella1_valoracionPropia"
        app:layout_constraintEnd_toStartOf="@+id/imageView_estrella3_valoracionPropia"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_estrella1_valoracionPropia"
        app:layout_constraintTop_toTopOf="@+id/imageView_estrella1_valoracionPropia" />

    <ImageView
        android:id="@+id/imageView_estrella1_valoracionPropia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@android:drawable/btn_star"
        app:layout_constraintBottom_toBottomOf="@+id/imageView_perfilUsuarioImagen"
        app:layout_constraintEnd_toStartOf="@+id/imageView_estrella2_valoracionPropia"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_perfilUsuarioImagen"
        app:layout_constraintTop_toTopOf="@+id/imageView_perfilUsuarioImagen" />

    <ImageView
        android:id="@+id/imageView_estrella4_valoracionPropia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@android:drawable/btn_star"
        app:layout_constraintBottom_toBottomOf="@+id/imageView_estrella3_valoracionPropia"
        app:layout_constraintEnd_toStartOf="@+id/imageView_estrella5_valoracionPropia"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_estrella3_valoracionPropia"
        app:layout_constraintTop_toTopOf="@+id/imageView_estrella3_valoracionPropia" />

    <ImageView
        android:id="@+id/imageView_estrella5_valoracionPropia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@android:drawable/btn_star"
        app:layout_constraintBottom_toBottomOf="@+id/imageView_estrella4_valoracionPropia"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView_estrella4_valoracionPropia"
        app:layout_constraintTop_toTopOf="@+id/imageView_estrella4_valoracionPropia" />

    <ImageView
        android:id="@+id/imageView_perfilUsuarioImagen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:src="@tools:sample/avatars" />
</androidx.constraintlayout.widget.ConstraintLayout>